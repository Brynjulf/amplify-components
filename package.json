{
  "name": "@equinor/amplify-components",
  "version": "1.5.2",
  "description": "Frontend Typescript components for the Amplify team",
  "files": [
    "dist/*"
  ],
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "sideEffects": false,
  "scripts": {
    "start": "yarn storybook",
    "build": "yarn build-storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "setup": "yarn",
    "pretty": "prettier --check ./src",
    "pretty:fix": "prettier --write ./src",
    "lint": "tsc --noEmit && eslint ./src --ext .jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .jsx,.ts,.tsx --fix",
    "test": "run-script-os",
    "test:win32": "set CI=true&react-scripts test --colors",
    "test:macos": "CI=true react-scripts test",
    "test:watch": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "setup-husky": "husky install",
    "precommit": "yarn && node tooling/commit-tasks.mjs",
    "clean": "rm -rf lib dist",
    "build-components": "yarn clean && rollup -c rollup.config.js",
    "build-components:ci": "rm -rf lib dist && rollup -c rollup.config.js",
    "version": "version-bump cli --bump",
    "postversion": "git add package.json && git commit -m 'Upgraded package version'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "serialize-javascript": ">=3.1.0",
    "trim": ">=0.0.3",
    "glob-parent": ">=5.1.2",
    "nth-check": ">=2.0.1"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ]
  },
  "dependencies": {
    "@azure/msal-browser": "^2.23.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@equinor/eds-core-react": "^0.18.0",
    "@equinor/eds-icons": "^0.10.0",
    "@equinor/eds-tokens": "^0.7.0",
    "@mui/material": "^5.6.0",
    "@mui/styles": "^5.6.0",
    "downshift": "^6.1.7",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "random-seed": "^0.3.0",
    "react-dropzone": "^12.0.4",
    "styled-components": "^5.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@open-wc/building-rollup": "^2.0.1",
    "@optimize-lodash/rollup-plugin": "^3.0.0",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.19",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@theo.gravity/version-bump": "^2.0.14",
    "@types/jest": "^27.4.1",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.24",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@types/uuid": "^8.3.4",
    "audit-ci": "^6.1.2",
    "babel-preset-minify": "^0.5.1",
    "chalk": "^5.0.1",
    "child_process": "^1.0.2",
    "eslint-plugin-jest-dom": "^4.0.1",
    "husky": "^7.0.2",
    "ora": "^6.0.1",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.0",
    "rollup": "^2.70.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-typescript-paths": "^1.3.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-uglify": "^6.0.4",
    "run-script-os": "^1.1.6",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "styled-components": ">=4.2",
    "webpack": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/amplify-components.git"
  },
  "author": "Amplify Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/equinor/amplify-components/issues"
  },
  "homepage": "https://github.com/equinor/amplify-components#readme"
}
