{
  "name": "@equinor/amplify-components",
  "version": "2.0.6",
  "description": "Frontend Typescript components for the Amplify team",
  "files": [
    "dist/*"
  ],
  "main": "dist/amplify-components.cjs.js",
  "publishConfig": {
    "main": "dist/amplify-components.cjs.js",
    "module": "dist/esm/index.js"
  },
  "types": "dist/types/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "sideEffects": false,
  "scripts": {
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook",
    "setup": "yarn",
    "pretty": "prettier --check ./src",
    "pretty:fix": "prettier --write ./src",
    "lint": "tsc --noEmit && eslint ./src --ext .jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .jsx,.ts,.tsx --fix",
    "test": "run-script-os",
    "test:win32": "set CI=true&react-scripts test --colors",
    "test:macos": "CI=true react-scripts test",
    "test:watch": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "setup-husky": "husky install",
    "precommit": "yarn && node tooling/commit-tasks.mjs",
    "build": "yarn build-components",
    "build-components": "rollup -c rollup.config.js && tsc -p tsconfig.build.json",
    "build-components:ci": "rm -rf lib dist && rollup -c rollup.config.js && tsc -p tsconfig.build.json",
    "version": "version-bump cli --bump",
    "postversion": "git add package.json && git commit -m 'Upgraded package version'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@azure/msal-browser": "^2.28.1",
    "@babel/runtime": "^7.18.9",
    "html2canvas": "^1.4.1",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.8.0",
    "proj4": "^2.8.0",
    "random-seed": "^0.3.0",
    "react-dropzone": "^14.2.2",
    "react-hook-form": "^7.34.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@equinor/eds-core-react": "^0.20.4",
    "@faker-js/faker": "^7.4.0",
    "@open-wc/building-rollup": "^2.0.2",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@theo.gravity/version-bump": "^2.0.14",
    "@types/jest": "^28.1.7",
    "@types/leaflet": "^1.7.11",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.12",
    "@types/proj4": "^2.5.2",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "audit-ci": "^6.3.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-preset-minify": "^0.5.2",
    "chalk": "^5.0.1",
    "child_process": "^1.0.2",
    "eslint": "^8.22.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "ora": "^6.1.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rollup": "^2.78.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "run-script-os": "^1.1.6",
    "styled-components": "^5.3.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    }
  },
  "peerDependencies": {
    "@equinor/eds-core-react": "^0.20.4",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "styled-components": ">=4.2",
    "webpack": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equinor/amplify-components.git"
  },
  "author": "Amplify Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/equinor/amplify-components/issues"
  },
  "homepage": "https://github.com/equinor/amplify-components#readme"
}
